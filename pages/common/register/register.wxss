page{background:#f7f7f7; padding-top:30rpx;}
.form_mdl{border-radius:0;}
.form_mdl .get_code_dl{padding:15rpx 0; height:70rpx;}
.form_mdl .get_code_dl .dt,
.form_mdl .get_code_dl .dd,
.form_mdl .get_code_dl .dd .input{height:70rpx; line-height:70rpx;}
.form_mdl .get_code_btn{width:200rpx; position:absolute; top:0; right:10rpx; z-index: 99; }

.tip_txt{font-size:28rpx; color:#eb0000; text-align: left; padding:20rpx 20rpx;line-height:30rpx;}
.btn_wrap{position:fixed; bottom:100rpx; left:0; width:100%; box-sizing:border-box; padding:0 100rpx;}

.mask_bg_ctn{height:100%;}
.garden_mdl{width:90%; height:900rpx; border-radius:20rpx; margin:0 auto; overflow:auto; text-align: left; vertical-align: top; background:#f7f7f7; position:relative;}
.garden_mdl .close_btn{width:50rpx; height:50rpx; background-position: -250rpx -60rpx; position:absolute; right:15rpx; top:15rpx;}
.garden_list{height:750rpx; overflow:auto;}
/*标题*/
.allpark_title{font-size:26rpx;font-weight:bold;color:#888;padding:40rpx 0 20rpx 25rpx; line-height:40rpx;}
/*园区01*/
.allpark01{padding:0 20rpx;background:#fff; position:relative; margin-bottom:20rpx;}
.allpark01 .top{padding:30rpx 10rpx;}
.allpark01 .top .icon{float:left; width:30rpx; margin-right:10rpx;}
.allpark01 .top .info{overflow:hidden;}
.allpark01 .top .info .title{font-size:32rpx; color:#333; margin-bottom:10rpx;}
.allpark01 .top .info .address{font-size:28rpx; color:#888;}
.allpark01 .radio_box{height:80rpx; color:#888888;}
.allpark01 .radio_box radio{transform:scale(.7); font-size:40rpx; text-indent:10rpx;}
.allpark01 .gou{display:inline-block; width:60rpx; height:60rpx; background:no-repeat center top url(https://www.5iparks.com/static/yuanding/images/icon/gou.png);background-size: 100% 100%; right:0; bottom:0;position:absolute}

.company_li{ line-height:50rpx; padding:15rpx 30rpx; border-bottom:1rpx solid #e5e5e5; font-size:30rpx; color:#333; background:#fff; }

.login_btm{overflow: hidden; font-size:28rpx;padding:20rpx 20rpx; color:#b2b2b2;}
.login_btm .forget_btn{float:right; color:#b2b2b2; }
.login_btm .reg_btn{float:left;}
.login_btm .reg_btn .a{display:inline-block; color:#2a99f1; text-decoration:underline;}

.search_input_box{padding:20rpx 30rpx; background:#fff;}
.serach_input{width:100%; height:50rpx; line-height:50rpx; border:1rpx solid #ddd;  box-sizing: border-box; padding:0 20rpx; text-align: center; border-radius:25rpx; font-size:24rpx;}
.wx_btn{width:100%; height:100%; background:#000; position:absolute; left:0; top:0; z-index:99; opacity: 0;}