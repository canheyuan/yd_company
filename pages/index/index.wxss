
.index_ico{display:inline-block; background:url(https://www.5iparks.com/static/yuanding/images/index/index_icons.png) no-repeat; background-size:300rpx 300rpx;}
.index_title{display: block;  margin-bottom:20rpx; font-weight: bold;}
.index_title text{color:#000; font-size:34rpx; display: block; overflow: hidden;}
.index_title .more_btn{float:right;color:#2a99f1; font-size:26rpx; padding:6px 0 0 20px; }
.index_title .more_btn.hover{background:none; color:#2b7ec0;}

/* -------- index 顶部 ------- */
.index_mdl01{position:relative; height:456rpx;  overflow: hidden; background:url(https://www.5iparks.com/static/yuanding/images/index/top_bg.jpg) no-repeat center top; background-size:100% auto;}
/* 顶部 */
.index_top{position:relative; z-index: 9; padding:30rpx 25rpx 20rpx; }
.index_top .msg_btn{float:right; position:relative;  margin-left:40rpx; width:50rpx; height:50rpx; background-position:-50rpx -60rpx;}
.index_top .msg_btn .num{background:#ff3b30; display: inline-block; width:30rpx; height:30rpx;text-align: center; line-height:30rpx; position:absolute; top:-12rpx; right:-12rpx; border-radius:15rpx; color:#fff; font-size:24rpx;}
.index_top .search_btn{float:right;width:50rpx; height:50rpx; background-position:-0 -60rpx; }
.index_top .search_btn .img{display:inline-block; width:40rpx; height:40rpx;}
.index_top .address{color:#fff; overflow: hidden; font-size:24rpx;}
.index_top .address .ico_dz{ width:50rpx; height:50rpx; vertical-align: middle; position:relative; top:-3rpx; background-position:-100rpx -60rpx;}
.index_top .address .txt{ font-size:36rpx; line-height:50rpx;}
/* 幻灯片 */
.index_slide_box{position:relative; z-index: 9; border-radius: 0rpx;  overflow:hidden;}
.index_slide{width:100%; height:340rpx; overflow: hidden; border-radius: 0rpx; }
.index_slide .slide_img{width:730rpx; height:320rpx; border-radius:20rpx; margin:0 10rpx; box-shadow: 0 5rpx 20rpx rgba(0,0,0,.24);}

.index_mdl02{background:#fff; box-shadow:0 0 24rpx 5rpx #eeeeee; border-radius:20rpx;  background:#fff;padding:20rpx; position:relative; padding-bottom:50rpx;}
/* ------------- 通知公告 ------------- */
.notice_mdl{padding:20rpx; background:#f5f5f5; overflow: hidden; border-radius:15rpx;}
.notice_mdl .notice_img{float:left; width:96rpx; height:80rpx; margin-right:20rpx;}
.notice_mdl .notice_ctn{overflow: hidden;}
.notice_slide{height:40rpx;}
.notice_slide .label{ width:70rpx;text-align: center; height:30rpx; line-height:30rpx; font-size:24rpx; background:#fa7947; border-radius:6rpx;  color:#fff; float:left; margin-right:20rpx; margin-top:5rpx;}
.notice_slide .l_orange{background:#fa7947;}
.notice_slide .l_blue{background:#6592fe;}
.notice_slide .a{display:block; overflow: hidden; font-size:26rpx; white-space:nowrap; text-overflow:ellipsis; color:#555555; line-height:40rpx;}
/* 菜单 */
.menu_list{padding:10rpx 0 0;  overflow: hidden; transition:all .3s;}
.menu_list .a{display:block; width:25%; text-align: center; float:left; padding:15rpx 0; height:160rpx;}
.menu_list .a .icon{display:block; width:130rpx; height:130rpx; margin:0 auto; background:url(https://www.5iparks.com/static/yuanding/images/index-menu-icons.png) no-repeat; background-size:650rpx 520rpx;}
.menu_list .a .icon.ico01{background-position:0rpx 0rpx;}
.menu_list .a .icon.ico02{background-position:-130rpx 0rpx;}
.menu_list .a .icon.ico03{background-position:-260rpx 0rpx;}
.menu_list .a .icon.ico04{background-position:-390rpx 0rpx;}
.menu_list .a .icon.ico05{background-position:-520rpx 0rpx;}
.menu_list .a .icon.ico06{background-position:0rpx -130rpx;}
.menu_list .a .icon.ico07{background-position:-130rpx -130rpx;}
.menu_list .a .icon.ico08{background-position:-260rpx -130rpx;}
.menu_list .a .icon.ico09{background-position:-390rpx -130rpx;}
.menu_list .a .icon.ico10{background-position:-520rpx -130rpx;}
.menu_list .a .icon.ico11{background-position:0rpx -260rpx;}
.menu_list .a .icon.ico12{background-position:-130rpx -260rpx;}
.menu_list .a .icon.ico13{background-position:-260rpx -260rpx;}
.menu_list .a .icon.ico14{background-position:-390rpx -260rpx;}
.menu_list .a .title{font-size:26rpx; color:#333333; display: block;}
.menu_list .a.hover .title{color:#2a99f1;}
.more_menu{position:absolute; bottom:0; left:50%; margin-left:-45rpx; background:#f0f0f0; width:90rpx; height:50rpx; border-top-left-radius: 45rpx; border-top-right-radius: 45rpx; text-align: center; padding-top:10rpx; box-sizing: border-box;}
.more_menu .ico_more{width:40rpx; height:40rpx; background-position: -410rpx -370rpx; transition:all .3s;}
.more_menu .ico_more.up{transform:rotate(180deg);}




.index_mdl03{padding:20rpx 20rpx 0; margin-top:30rpx;}
/* 待办事项 */
.dbsx_list{background:#fff; box-shadow:0 0 24rpx 5rpx #eeeeee; border-radius:10rpx;  background:#fff;padding:60rpx 20rpx 0rpx 120rpx;}
.dbsx_list .li{padding-bottom:20rpx; padding-left:30rpx; border-left:1rpx solid #e2e2e2; position:relative;}
.dbsx_list .li:last-child{border-left:0;}
.dbsx_list .li .time{position:absolute; left:-100rpx; top:0; width:70rpx; color:#999; font-size:20rpx; text-align: right;}
.dbsx_list .li .time text{display:block; font-size:24rpx;}
.dbsx_list .li .dot{display:inline-block; border:4rpx solid #fff; background:#e5e5e5; padding:3rpx; width:18rpx; height:18rpx; border-radius:100%; position:absolute; left:-13rpx; top:0;}
.dbsx_list .li .dot .dot_in{display:inline-block; border:5rpx solid #fec92f; background:#fff; height:8rpx; width:8rpx; border-radius:100%; position:absolute; left:3rpx; top:3rpx;}
.dbsx_list .li .info{background:#f5f5f5; padding:15rpx; border-radius:10rpx; position:relative; top:-20rpx;}
.dbsx_list .li .info .title{font-size:30rpx; color:#333; margin-bottom:10rpx;}
.dbsx_list .li .info .title .label{display:inline-block; height:30rpx; line-height:30rpx; padding:0 10rpx; background:#fec92f; color:#fff; font-size:20rpx; border-radius:6rpx; margin-left:10rpx; vertical-align: middle;}
.dbsx_list .li .info .price{color:#9d9d9d; font-size:26rpx;}
.dbsx_list .li .info .pay_status{display:inline-block; height:30rpx; line-height:30rpx; background:#d6d6d6; font-size:20rpx; color:#fff; padding:0 10rpx; border-radius:6rpx;}
.dbsx_list .li .info .arrow_ico{display:inline-block; width:50rpx; height:50rpx; background-position:-198rpx -60rpx; position:absolute; right:20rpx; top:50%; margin-top:-25rpx;}
/* .dbsx_list .li_blue .info .title .label{background:#36c3fe;}
.dbsx_list .li_blue .dot .dot_in{border-color:#36c3fe;} */

.dbsx_list .li_blue .info .title .label{background:#55b0bd;}
.dbsx_list .li_blue .dot .dot_in{border-color:#55b0bd;}
.dbsx_list .li_blue .info{border-top: solid 6rpx #55b0bd;}
.dbsx_list .li_green .info{border-top: solid 6rpx #54bb54;}
.dbsx_list .li_green .info .title .label{background: #54bb54;}
.dbsx_list .li_green .dot .dot_in{border-color:#54bb54;}
.dbsx_list .li_purple .info{border-top: solid 6rpx #676ec6;}
.dbsx_list .li_purple .info .title .label{background: #676ec6;}
.dbsx_list .li_purple .dot .dot_in{border-color: #676ec6;}
.dbsx_list .li_red .info{border-top: solid 6rpx #d63a31;}
.dbsx_list .li_red .info .title .label{background: #d63a31;}
.dbsx_list .li_red .dot .dot_in{border-color: #d63a31;}


/* 申报政策 */
.index_mdl04{padding:20rpx 0 0; margin-top:30rpx;}
.index_mdl04 .index_title, .index_mdl06 .index_title{margin-left:22rpx; margin-right: 24rpx;}
.policy_list_box{height:470rpx;}
.policy_list{height:100%;}
.policy_list .li{width:410rpx; height:450rpx; float:left; margin-left: 20rpx; border-radius:10rpx; overflow: hidden; background:#fff; box-shadow: 0 5rpx 24rpx rgba(0,0,0,.1);}
.policy_list .li .img{width:100%; height:285rpx;}
.policy_list .li .info{padding:10rpx 15rpx;}
.policy_list .li .info .title{display:block; font-size:30rpx; line-height:40rpx; margin-bottom:15rpx; height:80rpx;display: -webkit-box; -webkit-box-orient: vertical; overflow: hidden; -webkit-line-clamp: 2;}
.policy_list .li .info .btm{overflow:hidden; font-size:24rpx; color:#9d9d9d; }
.policy_list .li .info .btm>view{display: inline-block;  width:50%; line-height:30rpx; text-overflow:ellipsis; white-space:nowrap; overflow:hidden;}
.policy_list .li .info .btm>view icon{width:30rpx; height:30rpx; margin-right:7rpx; vertical-align: middle; position:relative; top:-2rpx; }
.policy_list .li .info .btm>view icon.ico_cgl{background-position:-30rpx -120rpx;}
.policy_list .li .info .btm>view icon.ico_zg{background-position:0 -120rpx;}
.policy_list .li .info .btm>view text{color:#ff6000; }

/* 热门活动 */
.index_mdl06{padding:4rpx 0 0; margin-top:30rpx;}
.hot_act_list_box{height:400rpx;}
.hot_act_list {height:100%;}
.hot_act_list .li{width:300rpx; height:380rpx;  float:left; margin-left: 20rpx; border-radius:10rpx; background:#fff; box-shadow: 0 5rpx 24rpx rgba(0,0,0,.1);}
.hot_act_list .li .img{width:100%; height:227rpx;  border-top-left-radius: 10rpx; border-top-right-radius: 10rpx;}
.hot_act_list .li .info{padding:5rpx 15rpx;}
.hot_act_list .li .info .title{font-size:30rpx;line-height:40rpx; margin-bottom:15rpx; height:80rpx;display: -webkit-box; -webkit-box-orient: vertical; overflow: hidden; -webkit-line-clamp: 2;}
.hot_act_list .li .info .btm{overflow:hidden; font-size:24rpx; color:#9d9d9d;}
.hot_act_list .li .info .btm>view{display: inline-block;  width:50%;}
.hot_act_list .li .info .btm>view icon{width:30rpx; height:30rpx; margin-right:7rpx; vertical-align: middle; position:relative; top:-2rpx; }
.hot_act_list .li .info .btm>view icon.ico_eye{background-position:-90rpx -120rpx;}
.hot_act_list .li .info .btm>view icon.ico_msg{background-position:-60rpx -120rpx;}
.hot_act_list .li .info .btm>view text{color:#999;}

.index_mdl05{padding:6rpx 20rpx 40rpx; margin-top:30rpx;}
/* 线上图书 */
.xsts_ctn{padding:18rpx; background:#fff;box-shadow:0 0 24rpx 5rpx #eeeeee; border-radius:10rpx;}
.xsts_list{font-size:0; background:#fff; width:678rpx; overflow: hidden; border-radius:10rpx;}
.xsts_list .a{float:left; width:339rpx; height:205rpx;}
.xsts_list .a .img{width:339rpx; height:205rpx; }

.back_top{width:80rpx; height:80rpx; text-align: center; background:#fff; border:4rpx solid #dcdcdc; border-radius:100%; position:fixed; bottom:20rpx; right:20rpx; z-index:99; box-sizing:border-box;}
.back_top .ico_top{width:50rpx; height:50rpx; background-position: -146rpx -64rpx; margin-top:15rpx;}
